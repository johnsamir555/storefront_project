  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "start:prod": "npm run build && node dist/server.js",
    "migrate:up": "db-migrate up",
    "migrate:down": "db-migrate down",
    "migrate:reset": "db-migrate reset",
    "migrate_test:up": "db-migrate -e test up",
    "migrate_test:down": "db-migrate -e test down",
    "migrate_test:reset": "db-migrate -e test reset",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "format": "prettier src/**/*.ts",
    "format:fix": "prettier --write src/**/*.ts",
    "test": "npm run build && set ENV=test && db-migrate up --env test && jasmine && db-migrate down"
  },
  "author": "",
  "license": "ISC",
